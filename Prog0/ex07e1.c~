#include<stdio.h>

int main() {
  int Fibonacchi[21];
  double goldenratio=1.6180339887;
  Fibonacchi[0]=0;
  Fibonacchi[1]=1;
  for(int i=2;i<21;i++){
    Fibonacchi[i]=Fibonacchi[i-1]+Fibonacchi[i-2];
  }
  for(int i=0;i<21;i++){
    printf("Fibonacchi[%d] = %d\n",i,Fibonacci[i]);
  }
  for(int i=1;i<20;i++){
    printf("Fibonachi[%d]/Fibonacchi[%d] - goldenratio = %9.7e\n",i+1,i,(double)Fibonacchi[i+1]/(double)Fibonacchi[i]-goldenratio);
  }
  return 0;
}
